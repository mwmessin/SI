  constructor: -> 
    @element = $ '<div>'
      .addClass 'slideshow'
      .appendTo 'body'

    for slide in @slides
      $ '<img>'
        .addClass 'slide'
        .attr 'data-src', "slides/#{slide}"
        .load @loadNext
        .appendTo @element

    $('.slide')[0].src = $('.slide')[0].attr('data-src')

  loadNext: ({target}) =>
    log target.attr('data-src')

    next = target.next()

    if next.src == ''
      next.load @loadNext
      next.src = next.attr('data-src')
    else
      $('.slide')[0].addClass 'current'
      @next.interval 4000

  next: =>
    $ '.current'
      .next()
      .addClass 'sliding'

    @end.timeout 1000

  end: =>
    $ '.current'
      .removeClass 'current'

    $ '.sliding'
      .removeClass 'sliding'
      .addClass 'current'

new class Playlist
  songs: [
    '01 - My Name Is Jonas.mp3'
    'Wonderful Tonight.mp3'
    '72 - The Cranberries - Dreams [1994].mp3'
    'Bridge Over Troubled Water.mp3'
    'Tupac - Dear Momma.mp3'
    '05 - Undone - The Sweater Song.mp3'
    'I Hope You Dance.mp3'
    'Crazy Love.mp3'
  ]

  constructor: -> 
    for song in @songs
      $ '<audio>'
        .attr 'data-src', "songs/#{song}"
        .ended @next
        .error @next
        .appendTo 'body'

    first = $('audio')[0]
    first.src = first.attr('data-src')
    first
      .addClass 'playing'
      .play()

  next: =>
    next = $ '.playing'
      .removeClass 'playing'
      .next('audio')[0]

    next.src = next.attr('data-src')
    next
      .addClass 'playing'
      .play()
